{
  "name": "aef-portail-agent",
  "version": "1.111.0",
  "pre-commit": [
    "lint",
    "random"
  ],
  "scripts": {
    "start": "ng serve --baseHref /sejour/",
    "start:sejour": "ng serve",
    "start:aot": "ng serve --aot",
    "build": "npm run build:prod",
    "build:dev": "ng build",
    "build:qualif": "ng build --build-optimizer --configuration=qualif",
    "build:prod": "ng build --build-optimizer --configuration=prod",
    "build:preprod": "ng build --build-optimizer --configuration=preprod",
    "lint": "npm run lint-ts && npm run lint-css",
    "lint-ts": "tslint --fix \"src/**/*.ts\"",
    "lint-css": "stylelint --fix \"src/**/*.scss\" --syntax scss",
    "pretest": "npm run lint",
    "test": "ng test",
    "test:coverage": "ng test --coverage --runInBand",
    "test:watch": "ng test --watch",
    "test:clearCache": "jest --clearCache",
    "test:codeship": "npm run lint && ng test --maxWorkers=4 --forceExit",
    "deploy": ""
  },
  "engines": {
    "node": ">=12.13.0 <13.0.0",
    "npm": ">=6.12.0 <=6.14.8"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~7.2.15",
    "@angular/compiler": "~7.2.15",
    "@angular/core": "~7.2.15",
    "@angular/forms": "~7.2.15",
    "@angular/platform-browser": "~7.2.15",
    "@angular/platform-browser-dynamic": "~7.2.15",
    "@angular/router": "~7.2.15",
    "@fullcalendar/angular": "4.4.2",
    "@fullcalendar/core": "4.4.2",
    "@fullcalendar/daygrid": "4.4.2",
    "@fullcalendar/interaction": "4.4.2",
    "@fullcalendar/list": "4.4.2",
    "@fullcalendar/timegrid": "4.4.2",
    "@hallysonh/ngx-imageviewer": "2.0.4",
    "anef-ui": "ssh://github.com:MI-ANEF/anef-ui-lib.git#develop",
    "bootstrap-scss": "^4.6.0",
    "file-saver": "2.0.0",
    "fullcalendar": "^4.0.0-alpha.4",
    "hammerjs": "^2.0.8",
    "jsbarcode": "3.8.0",
    "jspdf": "1.3.5",
    "lodash": "^4.17.21",
    "material-icons": "^0.3.1",
    "moment": "2.24.0",
    "moment-timezone": "0.5.23",
    "ng-bullet": "^1.0.3",
    "ng2-file-upload": "1.3.0",
    "pdfjs-dist": "2.3.200",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.4.4",
    "@angular-devkit/build-angular": "0.13.9",
    "@angular/cli": "7.1.3",
    "@angular/compiler-cli": "~7.2.15",
    "@angular/language-service": "~7.2.15",
    "@types/jest": "^24.9.1",
    "@types/jspdf": "1.3.3",
    "@types/lodash": ">=4.14.136 <4.14.170",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jest": "^24.8.0",
    "jsdom": "11.11.0",
    "lint-staged": "^9.2.1",
    "pre-commit": "^1.2.2",
    "protractor": "~5.4.0",
    "sass": "^1.45.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.2.0"
  }
}
